{
  "name": "16.7",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js docs",
    "init:files": "touch README.md index.html styleguide.html sass/style.scss sass/_mixins.scss sass/_layout.scss sass/_normalize.scss sass/_variables.scss js/script.js css/style.css images vendor",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build-dev": "npm-run-all build:clean build:sass build:autoprefixer test",
    "build:clean": "rimraf css/* && rimraf docs/*",
    "build:sass": "node-sass --output-style compressed -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build:copy": "npm-run-all copy:*",
    "build:tinypng": "tinypng docs/assets/images",
    "build:uglify": "uglifyjs --compress --mangle --output docs/js/script.js -- js/script.js",
    "build:html-minifier": "html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true index.html -o docs/index.html",
    "copy:css": "cp -r css docs",
    "copy:js": "cp -r js docs",
    "copy:fonts": "cp -r fonts docs",
    "copy:images": "cp -r images docs",
    "copy:htmls": "cp -r *html docs",
    "watch": "npm-run-all build-dev -p watch:*",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch:autoprefixer": "autoprefixer-cli css/style.css",
    "watch:browsersync": "browser-sync start --server --browser chrome --files css/*.css *html js/*.js",
    "watch:sassprefixer": "onchange sass/*.scss -- npm-run-all watch:sass watch:autoprefixer"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "html-minifier": "^4.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "tinypng-cli": "0.0.7",
    "uglify-js": "^3.5.15"
  }
}
